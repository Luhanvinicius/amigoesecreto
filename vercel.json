{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/form_layouts/(.*)",
      "dest": "/public/form_layouts/$1"
    },
    {
      "src": "/module_assets/(.*)",
      "dest": "/public/module_assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}


